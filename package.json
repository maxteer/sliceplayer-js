{
  "name": "sliceplayer-js",
  "version": "1.0.3",
  "description": "Audio player library for Discord.js.",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "tests"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "prism-media": "^1.3.1"
  },
  "devDependencies": {
    "@discordjs/opus": "^0.5.3",
    "babel-eslint": "^10.1.0",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "ffmpeg-static": "^4.3.0",
    "prettier": "^2.3.0",
    "sucrase": "^3.18.1"
  },
  "scripts": {
    "test": "node -r sucrase/register tests/index",
    "build": "sucrase ./src -d ./lib --transforms imports",
    "example-load-item": "node -r sucrase/register examples/loadItem",
    "example-convert-video": "node -r sucrase/register examples/convertVideoToOpusStream",
    "example-discord-bot": "node -r sucrase/register examples/discordBot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxteer/sliceplayer-js.git"
  },
